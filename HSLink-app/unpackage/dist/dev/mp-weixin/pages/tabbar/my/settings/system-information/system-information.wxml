<view class="page"><view class="cu-list menu sm-border"><block wx:for="{{systemInfo}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="cu-item arrow"><view class="content"><text class="text-grey">{{item.key}}</text></view><block wx:if="{{typeof item.value!=='object'}}"><view class="action"><text class="text-grey text-sm">{{item.value}}</text></view></block><block wx:if="{{typeof item.value==='object'}}"><view class="action"><block wx:for="{{item.value}}" wx:for-item="value" wx:for-index="key" wx:key="*this"><view class="text-grey text-sm">{{ATTRIBUTE_CODE[key]+":"+value}}</view></block></view></block></view></block></view></view>